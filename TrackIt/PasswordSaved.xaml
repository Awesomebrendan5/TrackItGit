<Window x:Class="TrackIt.PasswordSaved"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrackIt"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="PasswordTooShort" Height="282" Width="461">
    <Canvas Focusable="true" Background="#75A0D8">
        <TextBlock Name="SaveMessage" Height="48" Width="257" Text="Password Saved" FontFamily="Narkisim" FontSize="30" Foreground="Black" Canvas.Left="102" HorizontalAlignment="Left" VerticalAlignment="Center" Canvas.Top="10" TextAlignment="Center"/>
        <TextBlock Name="Information" Height="38" Width="275" FontFamily="Narkisim" FontSize="20" Foreground="Black" Canvas.Left="93"  VerticalAlignment="Top" Canvas.Top="85" TextAlignment="Left" HorizontalAlignment="Center"><Run Text="Your password has been saved"/></TextBlock>
        <Button Name="Exit" Click="ExitButtonClick" Height="54" Width ="140" Canvas.Top="191" Background="#E14026" Foreground="#000000" BorderBrush="Transparent" BorderThickness="4" FontFamily="Calibri" FontSize="25" Content="Exit" HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.Left="160"/>
    </Canvas>
</Window>
